.mfp-figure { line-height: 0; }

.mfp-figure::after { content: ""; position: absolute; left: 0px; top: 40px; bottom: 40px; display: block; right: 0px; width: auto; height: auto; z-index: -1; box-shadow: rgba(0, 0, 0, 0.6) 0px 0px 8px; background: rgb(68, 68, 68); }

